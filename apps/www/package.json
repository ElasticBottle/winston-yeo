{
  "name": "www",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types --env-interface Env",
    "dev": "pnpm with-env-local vite",
    "build:preview": "pnpm with-env-preview vite build && tsc",
    "build:production": "pnpm with-env-prod vite build && tsc",
    "serve:preview": "pnpm with-env-preview vite preview",
    "serve:production": "pnpm with-env-prod vite preview",
    "test": "vitest run",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "pnpm content-collections build && tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run -f ../../.env.local  -f ../../.env -- ",
    "with-env-preview": "dotenvx run  -f ../../.env -- ",
    "with-env-prod": "dotenvx run -f ../../.env.production -- "
  },
  "dependencies": {
    "@rectangular-labs/api": "workspace:*",
    "@rectangular-labs/ui": "workspace:*",
    "@simplewebauthn/browser": "^13.1.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "@tanstack/react-router-with-query": "^1.130.12",
    "@tanstack/react-start": "^1.130.15",
    "arktype": "^2.1.20",
    "motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "wrangler": "^4.28.1"
  },
  "devDependencies": {
    "@content-collections/cli": "^0.1.7",
    "@content-collections/core": "^0.11.0",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/vite": "^0.2.7",
    "@rectangular-labs/typescript": "workspace:*",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "katex": "^0.16.22",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "web-vitals": "^5.0.3"
  }
}
